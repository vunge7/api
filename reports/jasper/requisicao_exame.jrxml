	<queryString language="SQL">
		<![CDATA[SELECT 
	l.id as id_linha,
	l.estado as estado_requisicao,
	l.hora as hora_requisicao,
	r.status as status_requisicao,
	f.nome as nome,
	f.nif as nif,
	f.endereco as endereco,
	f.telefone as telefone,
	p.nome  as nome_usuario,
	p2.nome as nome_usuario,
	p2.nome as nome_paciente,
	p2.apelido as apelido_paciente,
	p2.genero as sexo_paciente,
	p2.bairro as bairro_paciente,
	p2.raca as raca_paciente,
	r.data_requisicao as data_requisicao,
	pr.product_description as product_description, 
	pr.product_code as codigo_produto,
	pr.preco as preco_exame,
	l.exame as exames_requisitados
FROM linha_requisicao_exame l
    INNER JOIN requisicao_exame r ON r.id = l.requisicao_exame_id
        INNER JOIN usuario u ON u.id = r.usuario_id
        INNER JOIN funcionario f2 ON f2.id = u.funcionario_id 
        INNER JOIN pessoa p ON p.id  = f2.pessoa_id 
        INNER JOIN inscricao i ON  i.id  = r.inscricao_id 
        INNER JOIN paciente pa ON pa.id  = i.paciente_id 
        inner join pessoa p2 on p2.id = pa.pessoa_id 
        INNER JOIN produto pr ON pr.id = l.produto_id    
        JOIN filial f   
WHERE r.id = $P{id}]]>
	</queryString>

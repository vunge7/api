	<parameter name="id" class="java.lang.Long"/>
	<queryString language="SQL">
		<![CDATA[select 
	l.id as id_linha,
	l.medicamento as medicamento,
	l.dosagem as dosagem,
	l.posologia as posologia,
	l.quantidade as quantidade,
	l.via_administracao as via_administracao,
	l.frequencia as frequencia,
	pf.nome as nome_usuario,
	pp.nome as nome_paciente,
	pp.apelido as apelido_paciente,
	pp.genero as sexo_paciente,
	pp.bairro as bairro_paciente,
	pp.raca as raca_paciente,
	fl.nome as nome_filial,
	fl.nif  as nif,
	fl.endereco as endereco ,
	fl.telefone as telefone ,
	r.`data` as data_receita
from linha_receita l 
inner join receita r on r.id = l.receita_id
inner join usuario u on u.id = r.usuario_id
inner join funcionario f on f.id = u.funcionario_id
inner join pessoa pf on pf.id  = f.pessoa_id
inner join inscricao i on i.id = r.inscricao_id 
inner join paciente pa on pa.id = i.paciente_id
inner join pessoa pp on pp.id  = pa.id 
join filial fl 
 WHERE r.id =$P{id};]]>
	</queryString>
